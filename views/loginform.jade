extends layout

block head  
	title DVM - Sign Up Form

block content  
	.formcontent
		form(method='post',action='')
			label(for='user') *User:
			input(type='text',name='user',id='user')      
			label(for='pass') *Password:
			input(type='password',name='pass',id='pass')
			
			input(type='submit',name='submit',value='Submit').submit-button